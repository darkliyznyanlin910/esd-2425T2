{
  "name": "@repo/api",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./env": "./src/env.ts"
  },
  "scripts": {
    "build": "tsc",
    "start": "pnpm with-env tsx src/index.ts",
    "dev": "pnpm with-env tsx watch src/index.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "test": "jest --detectOpenHandles",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "clean": "git clean -xdf .cache .next .turbo node_modules dist"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.11.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.6",
    "dotenv-cli": "^8.0.0",
    "express": "^4.18.3",
    "morgan": "^1.10.0",
    "supertest": "^6.3.3",
    "tsx": "^4.7.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.16.5",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "typescript": "5.5.4"
  },
  "prettier": "@repo/prettier"
}
