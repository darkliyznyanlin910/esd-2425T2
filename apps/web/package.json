  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "typescript": "catalog:"
  },
  "prettier": "@repo/prettier"
